package com.demo.user.payloads.enums;

public enum Role_Permissions {

	READ("read"),
	WRITE("write"),
	UPDATE("update"),
	DELETE("delete");
	
	private final String permission;

	private Role_Permissions(String permission) {
		this.permission = permission;
	}

	public String getPermission() {
		return permission;
	}
	
}
