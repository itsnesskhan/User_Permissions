package com.demo.user.payloads.enums;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@AllArgsConstructor
@NoArgsConstructor
@Data
public enum Role_Permissions {

	READ("user::read"),
	WRITE("user::write"),
	UPDATE("user::update"),
	DELETE("delete");
	
	private final String permission;
	
	
	
}
